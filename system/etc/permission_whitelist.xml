<?xml version="1.0" encoding="utf-8"?>
<permission-whitelist version="1">
    <application>
        <!-- Note this!
         attribute:
           1. name: packagename.
           2. type: optional parameters. "system-app" - system app,
              "platform-app"- system app and signed by platform
           3. signature: optional parameters. public key to signed apk
         example:
         1. granted all runtime permissions to system app with packagename "com.android.test"
            <pkg name="com.android.test" type="system-app" />
         2. granted all runtime permissions to platform app with packagename "com.android.test"
            <pkg name="com.android.test" type="platform-app" />
         3. granted all runtime permissions to app with packagename "com.android.test"
            <pkg name="com.android.test"/>
         4. granted all runtime permissions to app with packagename "com.android.test"
            and public key "12345"
            <pkg name="com.android.test" signature="12345"/>
        -->

